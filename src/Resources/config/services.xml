<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Entity definitions -->
        <service id="Endereco\Shopware6Client\Entity\EnderecoAddressExtension\EnderecoAddressExtensionDefinition">
            <tag name="shopware.entity.definition"/>
        </service>

        <service id="Endereco\Shopware6Client\Entity\CustomerAddress\CustomerAddressExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Endereco\Shopware6Client\Controller\Api\ApiTestController" public="true">
            <argument type="service" id="Endereco\Shopware6Client\Service\EnderecoService" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="Endereco\Shopware6Client\Controller\Storefront\AddressController" public="true">
            <argument type="service" id="Endereco\Shopware6Client\Service\EnderecoService" />
            <argument type="service" id="customer_address.repository" />
            <argument type="service" id="event_dispatcher"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="Endereco\Shopware6Client\Service\EnderecoService" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="plugin.repository"/>
            <argument type="service" id="country_state.repository"/>
            <argument type="service" id="customer_address.repository"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcherInterface"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\AddressCheckPayloadBuilderInterface"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="Endereco\Shopware6Client\Run\Logger" />
        </service>

        <service id="Endereco\Shopware6Client\Service\AddDataToPage" >
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="Endereco\Shopware6Client\Service\EnderecoService" />
            <argument type="service" id="country.repository"/>
            <argument type="service" id="country_state.repository"/>
            <argument type="service" id="salutation.repository"/>
            <argument type="service" id="plugin.repository"/>
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="Endereco\Shopware6Client\Service\AddressCheck\AddressCheckPayloadBuilder" public="false">
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\LocaleFetcherInterface"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcherInterface"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\SubdivisionCodeFetcherInterface"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\CountryHasStatesCheckerInterface"/>
        </service>
        <service alias="Endereco\Shopware6Client\Service\AddressCheck\AddressCheckPayloadBuilder" id="Endereco\Shopware6Client\Service\AddressCheck\AddressCheckPayloadBuilderInterface" />

        <service id="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcher" public="false">
            <argument type="service" id="country.repository"/>
        </service>
        <service alias="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcher" id="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcherInterface" />

        <service id="Endereco\Shopware6Client\Service\AddressCheck\CountryHasStatesChecker" public="false">
            <argument type="service" id="country.repository"/>
        </service>
        <service alias="Endereco\Shopware6Client\Service\AddressCheck\CountryHasStatesChecker" id="Endereco\Shopware6Client\Service\AddressCheck\CountryHasStatesCheckerInterface" />

        <service id="Endereco\Shopware6Client\Service\AddressCheck\LocaleFetcher" public="false">
            <argument type="service" id="sales_channel_domain.repository"/>
        </service>
        <service alias="Endereco\Shopware6Client\Service\AddressCheck\LocaleFetcher" id="Endereco\Shopware6Client\Service\AddressCheck\LocaleFetcherInterface" />

        <service id="Endereco\Shopware6Client\Service\AddressCheck\SubdivisionCodeFetcher" public="false">
            <argument type="service" id="country_state.repository"/>
        </service>
        <service alias="Endereco\Shopware6Client\Service\AddressCheck\SubdivisionCodeFetcher" id="Endereco\Shopware6Client\Service\AddressCheck\SubdivisionCodeFetcherInterface" />

        <service id="Endereco\Shopware6Client\Run\LoggerFactory"/>

        <service id="Endereco\Shopware6Client\Run\Logger" class="Monolog\Logger">
            <factory service="Endereco\Shopware6Client\Run\LoggerFactory" method="createRotating"/>
            <argument type="string">endereco_shopware6_client</argument>
        </service>

        <service id="Endereco\Shopware6Client\Subscriber\AddressSubscriber">
            <tag name="kernel.event_subscriber"/>
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="Endereco\Shopware6Client\Service\EnderecoService" />
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="customer_address.repository"/>
            <argument type="service" id="endereco_address_ext.repository"/>
            <argument type="service" id="country.repository"/>
            <argument type="service" id="country_state.repository"/>
            <argument type="service" id="Endereco\Shopware6Client\Service\AddressCheck\CountryCodeFetcherInterface" />
            <argument type="service" id="request_stack"/>
        </service>
    </services>
</container>
